<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Language"
		items="{/Languages}"
		search="_handleValueHelpSearchlg"
		confirm="_handleValueHelpCloselg"
		cancel="_handleValueHelpCloselg">
		<StandardListItem			
			iconDensityAware="false"
			iconInset="false"
			title="{code}"
			description="{name}" />
	</SelectDialog>
</core:FragmentDefinition>