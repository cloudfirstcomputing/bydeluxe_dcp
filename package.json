  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy bydeluxe --delete-services --delete-service-keys --delete-service-brokers",
    "watch-distribution": "cds watch --open distribution/webapp/index.html?sap-ui-xx-viewCache=false",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-createdcpmaterials": "cds watch --open createdcpmaterials/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-booksalesorderweb": "cds watch --open booksalesorderweb/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-managematerialtitle": "cds watch --open managematerialtitle/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-adobeformprinting": "cds watch --open adobeformprinting/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-printdocuments": "cds watch --open printdocuments/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-dcpmatmap": "cds watch --open dcpmatmap/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-normalizedorder": "cds watch --open normalizedorder/webapp/index.html?sap-ui-xx-viewCache=false"
  },
