{
  "name": "bydeluxe",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.24.0",
    "@sap-cloud-sdk/odata-v2": "^3.24.0",
    "@sap/cds": "^8",
    "@sap/cds-common-content": "^2.1.0",
    "@sap/xsenv": "^5.3.0",
    "@sap/xssec": "^4",
    "express": "^4",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8.3.0",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy bydeluxe --delete-services --delete-service-keys --delete-service-brokers",
    "watch-distribution": "cds watch --open distribution/webapp/index.html?sap-ui-xx-viewCache=false",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "path": "/sap/opu/odata/sap/API_BUSINESS_PARTNER",
          "destination": "S4HC_DEV",
          "requestTimeout": 1000000
        }
      },
      "YY1_SHIPPINGCONDITION_CDS": {
        "kind": "odata-v2",
        "model": "srv/external/YY1_SHIPPINGCONDITION_CDS",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "path": "/sap/opu/odata/sap/YY1_SHIPPINGCONDITION_CDS",
          "destination": "S4HC_DEV",
          "requestTimeout": 1000000
        }
      },
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      }
    }
  },
  "sapux": [
    "app/distribution"
  ]
}
